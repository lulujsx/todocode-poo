package ejercicio_integrador;

public class Bulbasaur extends Pokemon implements IPlanta {
	
	public Bulbasaur() {
		
	}
	
	@Override
	public void atacarPlacaje() {
		System.out.println(this.nombre_pokemon + "uso placaje");
	}
	@Override
	public void atacarAraniazo() {
		System.out.println(this.nombre_pokemon + "uso araniazo");
	}
	@Override
	public void atacarMordisco() {
		System.out.println(this.nombre_pokemon + "uso mordisco");
	}
	@Override
	public void atacarParalizar() {
		System.out.println(this.nombre_pokemon + "uso paralizar");
		
	}
	@Override
	public void atacarDrenaje() {
		System.out.println(this.nombre_pokemon + "uso drenaje");		
	}
	@Override
	public void atacarHojaAfilada() {
		System.out.println(this.nombre_pokemon + "uso hoja afilada");
		
	}
	@Override
	public void atacarLatigoCepa() {
		System.out.println(this.nombre_pokemon + "uso latigo cepa");
	}
	
}
